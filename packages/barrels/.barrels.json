{
  "$schema": "./tools/configuration.json",

  "barrels": [{
    "directory": ["test"],
    "include": ["test"],
    "exclude": ["test"],
    "header": "@generated by @jojoxd/barrels",
    "extensions": [".js"],
    "emitExtensions": false,
    "emitAssertions": true
  }],

  "include": ["src/**/*.ts"],
  "exclude": ["src/**/*.spec.ts"],

  "cwd": ".",
  "dryRun": false,
  "emitAssertions": true,
  "emitExtensions": false,

  "extensions": [".ts"],

  "header": "@generated by @jojoxd/barrels",

  "location": "all",
  "target": "cjs"
}